{
  "compilerOptions": {
    "target": "esNext",
    "baseUrl": "src",
    "module": "esNext",
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "removeComments": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}